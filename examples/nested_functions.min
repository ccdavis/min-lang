// Example with nested function calls

func double(x: int): int {
	return x * 2;
}

func quad(x: int): int {
	return double(double(x));
}

func compute(x: int): int {
	var y: int = double(x);
	var z: int = quad(x);
	return y + z;
}

print("double(5) =", double(5));
print("quad(5) =", quad(5));
print("compute(5) =", compute(5));
