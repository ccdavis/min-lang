// Heavy recursive fibonacci benchmark
// This will do many recursive function calls to stress-test frame allocation

func fib(n: int): int {
	if n <= 1 {
		return n;
	}
	return fib(n - 1) + fib(n - 2);
}

// Calculate fib(30) multiple times to get measurable time
var sum: int = 0;
for var i: int = 0; i < 5; i = i + 1 {
	sum = sum + fib(30);
}
print("Result:", sum);
