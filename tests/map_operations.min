// Test comprehensive map operations

// Test 1: String keys, int values
var ages: map[string]int = map[string]int{
    "Alice": 30,
    "Bob": 25
};

print("Test 1: Basic string->int map");
print("Map length:", len(ages));
print("Alice's age:", ages["Alice"]);
print("Bob's age:", ages["Bob"]);

// Test 2: Add new entries
ages["Charlie"] = 35;
ages["David"] = 40;
ages["Eve"] = 28;
print("\nTest 2: After adding 3 entries");
print("Map length:", len(ages));
print("Charlie's age:", ages["Charlie"]);
print("David's age:", ages["David"]);
print("Eve's age:", ages["Eve"]);

// Test 3: Modify existing entries
ages["Alice"] = 31;
ages["Bob"] = 26;
print("\nTest 3: After modifying Alice and Bob");
print("Alice's new age:", ages["Alice"]);
print("Bob's new age:", ages["Bob"]);

// Test 4: Delete entries
delete(ages, "Charlie");
delete(ages, "Eve");
print("\nTest 4: After deleting Charlie and Eve");
print("Map length:", len(ages));

// Test 5: Int keys, string values
var names: map[int]string = map[int]string{
    1: "First",
    2: "Second",
    3: "Third"
};

print("\nTest 5: Int->string map");
print("Map length:", len(names));
print("ID 1:", names[1]);
print("ID 2:", names[2]);
print("ID 3:", names[3]);

// Test 6: Add to int-keyed map
names[4] = "Fourth";
names[5] = "Fifth";
print("\nTest 6: After adding to int-keyed map");
print("Map length:", len(names));
print("ID 4:", names[4]);
print("ID 5:", names[5]);

// Test 7: String keys, string values
var capitals: map[string]string = map[string]string{
    "USA": "Washington",
    "France": "Paris",
    "Japan": "Tokyo"
};

print("\nTest 7: String->string map");
print("Map length:", len(capitals));
print("USA capital:", capitals["USA"]);
print("France capital:", capitals["France"]);
print("Japan capital:", capitals["Japan"]);

// Test 8: Modify string->string map
capitals["Germany"] = "Berlin";
capitals["Italy"] = "Rome";
print("\nTest 8: After adding Germany and Italy");
print("Map length:", len(capitals));
print("Germany capital:", capitals["Germany"]);
print("Italy capital:", capitals["Italy"]);

// Test 9: Int keys, int values
var squares: map[int]int = map[int]int{
    1: 1,
    2: 4,
    3: 9,
    4: 16,
    5: 25
};

print("\nTest 9: Int->int map (squares)");
print("Map length:", len(squares));
print("Square of 1:", squares[1]);
print("Square of 3:", squares[3]);
print("Square of 5:", squares[5]);

// Test 10: Add to int->int map
squares[6] = 36;
squares[7] = 49;
squares[8] = 64;
print("\nTest 10: After adding more squares");
print("Map length:", len(squares));
print("Square of 6:", squares[6]);
print("Square of 7:", squares[7]);
print("Square of 8:", squares[8]);

// Test 11: Delete from int->int map
delete(squares, 2);
delete(squares, 4);
print("\nTest 11: After deleting keys 2 and 4");
print("Map length:", len(squares));

print("\n=== All map operations tests passed ===");
